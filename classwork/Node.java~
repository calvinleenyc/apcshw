public class Node{
    private String data;
    private Node next;
    public Node(String d){
	data = d;
	next = null;
    }
    public void setData(String d){
	data = d;
    }
    public void setNode(Node n){
	next = n;
    }
    public Node getNode(){
	return next;
    }
    public String getData(){
	return data;
    }
    public String toString(){
	return data;
    }
}